{
	"sgComment": "SG",
	"exampleIdentifier": "html_example",
	"sortCategories": true,
  "categoryRename": {
    "find": "/(^\\d+\\.\\s?)/g",
    "replace": ""
  },
	"sections": {
		"styles": "",
		"development": "Dev:",
    "Sass stuff": "Sass:"
	},
	"rootFolder": "./",
	"excludeDirs": [
		"target",
		"node_modules",
		".git"
	],
	"fileExtensions": {
		"scss": false,
		"sass": true,
		"less": true,
		"md": true,
		"css": true
	},
	"templateFile": "./template.hbs",
	"themeFile": "./theme-tocmenu.css",
	"htmlOutput": "./styleguide/styleguide.html",
	"jsonOutput": "styleguide/styleguide.json",
	"handlebarsPartials": {
		"sticky": "../../template/sticky.js"
	},
	"highlightStyle": "arduino-light",
	"highlightFolder": "../../node_modules/highlight.js/styles",
	"customVariables": {
		"pageTitle": "Style Guide",
    "tocMenu": true
	},
	"markedOptions": {
		"gfm": true,
		"breaks": true,
		"smartypants": true
	},
	"logging": {
    "prefix": "[SG]",
    "level": "minimal"
  }
}
